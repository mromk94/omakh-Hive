# Application
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Database
DATABASE_URL=postgresql://omk_user:omk_password@localhost:5432/omk_hive

# Redis
REDIS_URL=redis://localhost:6379/0

# Message Bus
MESSAGE_BUS_TYPE=redis
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# LLM Providers
DEFAULT_LLM_PROVIDER=gemini
GEMINI_API_KEY=your_gemini_api_key
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GROK_API_KEY=your_grok_api_key

# Google Cloud / Vertex AI
GCP_PROJECT_ID=your_gcp_project_id
GCP_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Blockchain
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_KEY
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
QUEEN_CONTROLLER_ADDRESS=

# Fetch.ai / ASI
FETCH_NETWORK=testnet
FETCH_MNEMONIC=your_fetch_mnemonic_here

# Security
API_KEY_HEADER=X-API-Key
ADMIN_API_KEYS=your_admin_key_1,your_admin_key_2

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Learning Function
LEARNING_ENABLED=true
LEARNING_STORAGE_BUCKET=omk-hive-learning-data
BIGQUERY_DATASET=omk_hive_learning
